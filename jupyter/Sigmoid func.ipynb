   "source": [
    "import math\n",
    "\n",
    "\n",
    "#  4x1 + 5x2 - 9\n",
    "\n",
    "def score(x1, x2):\n",
    "    return (4*x1) + (5*x2) - 9\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + math.exp(-x))\n",
    "\n",
    "def result(x1, x2):\n",
    "    print(sigmoid(score(x1,x2)))\n",
    "\n",
    "\n",
    "result(1,1)\n",
    "result(2,4)\n",
    "result(5, -5)\n",
    "result(-4, 5)\n",
    "\n",
    "\n"
   ]
